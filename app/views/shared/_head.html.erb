<title><%= title %></title>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<%== meta_data_tags %>
<%== stylesheet_tags %>
<%= csrf_meta_tag %>
<%= javascript_include_tag 'jquery-1.4.2.min', 'jquery.validate.min.js', 'rails', 'application', :cache => 'jquery-core' %>

<%= javascript_include_tag 'jquery.lightbox.min.js' %>
<%= stylesheet_link_tag 'jquery.lightbox' %>

<%= javascript_include_tag 'bitspudlo.js' %>
<%= stylesheet_link_tag 'bitspudlo' %>

<% unless I18n.locale.to_s[0..1] == 'en' %>
  <%= javascript_include_tag ['localization/messages_', I18n.locale.to_s[0..1], '.js'].join %>
<% end %>
<%= yield :head %>
